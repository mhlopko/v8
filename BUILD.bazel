# Just the public header files. Only for inclusion by subpackages of V8.
cc_library(
    name = "public_v8_header",
    srcs = glob(["include/*"]),
    visibility = [":users"],
)

filegroup(
    name = "public_v8_header_file",
    srcs = glob(["include/*"]),
    visibility = [":users"],
)

