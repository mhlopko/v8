# ------------------------------------------------------------------------------
# Libraries
#
# NOTE: These are libraries used internally by the build of the third_party/v8/v6_9
#       component. No other clients should directly depend on these internal
#       targets.

cc_library(
    name = "core_lib",
    srcs = [
        ":v8_base_files",
        ":v8_init_files",
        ":v8_initializers_files",
        ":v8_libbase_files",
        ":v8_libplatform_files",
        ":v8_libsampler_files",
        ":v8_nosnapshot_files",
        ":torque_generated_core_files",
    ],
    copts = COPTS + COPTS_TARGET_ARCH,
    includes = ["include"],
    linkopts = LINK_OPTS,
    nocopts = NO_COPTS,
    deps = select({
        "//tools/cc_target_os:android": [
            ":public_v8_header",
        ],
        "//conditions:default": [
            "//base",
            ":public_v8_header",
        ],
    }) + [":torque_generated_core"],
    alwayslink = 1,
    visibility = [":users"],
)

cc_library(
    name = "core_with_icu_lib",
    srcs = [
        ":v8_base_files",
        ":v8_icu_nosnapshot_files",
