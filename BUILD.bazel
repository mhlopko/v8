py_binary(
    name = "js2c_script",
    main = "g3tools/js2c.py",
    srcs = [
        "g3tools/js2c.py",
        "g3tools/jsmin.py",
    ],
    paropts = [
        "--strip",
        "--compile",
    ],
)

genrule(
    name = "js2c_icu",
    srcs = [
        ":js2c_files",
        "src/js/intl.js",
    ],
    outs = ["libraries_icu.cc"],
    cmd = "$(location :js2c_script) $@ CORE $(SRCS)",
    tools = [":js2c_script"],
)

vardef("PREDICTABLE", "--random-seed=314159265")

genrule(
    name = "run_mksnapshot_icu",
    outs = ["snapshot_icu.cc"],
    cmd = "$(location :mksnapshot_icu) " + varref("PREDICTABLE") + " --startup-src $@",
    tools = [":mksnapshot_icu"],
)

genrule(
    name = "run_mksnapshot_arm64",
    outs = ["snapshot_arm64.cc"],
    cmd = "$(location :mksnapshot_with_simulator_arm64) " + varref("PREDICTABLE") + " --startup-src $@",
    tools = [":mksnapshot_with_simulator_arm64"],
)

genrule(
    name = "unsupported_32_bit_snapshot",
    outs = ["unsupported_32_bit_snapshot.cc"],
    cmd = select({
        "//tools/target_cpu:haswell": "echo '' > $@",
        "//tools/target_cpu:k8": "echo '' > $@",
        "//tools/target_cpu:arm64-v8a": "echo '' > $@",
        "//tools/target_cpu:x86_64": "echo '' > $@",
        # Fail for 32-bit target CPUs. We cannot build 32-bit simulator based snapshot on 64-bit host.
