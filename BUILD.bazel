package(
    # Note: no-implicit-fallthrough only exists in clang. Blaze's standard
    # gcc-clang hybrid (which runs both) needs the flag to compile successfully
    # with clang. The Android config, however, is gcc-only. However, we do not
    # need to prefix with -Xclang-only since, as of gcc 4.4, unknown -Wno-*
    # flags are ignored. See https://gcc.gnu.org/gcc-4.4/changes.html
    default_copts = ["-Wno-implicit-fallthrough"],
    default_hdrs_check = "loose",
    default_visibility = [
        "//visibility:private",
    ],
    features = [
        "-layering_check",
        "-parse_headers",
    ],
)

package_group(
    name = "users",
    includes = [
        "//third_party/v8:security_critical_users",
        "//third_party/v8:users",
    ],
    packages = [
        "...",
    ],
)

# Allmost all of V8 is authored by Google, but there are a few 3rd-party files.
# The files have their own BUILD rules (named *_3rd_party) with the necessary
# licenses clause.
licenses(["notice"])

exports_files(["LICENSE"])

