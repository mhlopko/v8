cc_binary(
    name = "mksnapshot_icu",
    srcs = ["src/snapshot/mksnapshot.cc"],
    copts = COPTS + COPTS_TARGET_ARCH + ["-DV8_INTL_SUPPORT"],
    linkopts = LINK_OPTS,
    nocopts = NO_COPTS,
    deps = [":core_with_icu_lib"],
)

cc_binary(
    name = "mksnapshot_with_simulator_arm64",
